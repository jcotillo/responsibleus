<%= form_for(@event) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :starts_at %>
  <%= f.datetime_select :starts_at %>

  <%= f.label :ends_at %>
  <%= f.datetime_select :ends_at %>

  <%= f.submit %>
<% end %>
